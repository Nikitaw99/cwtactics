class German_Lang(LanguageCard) :
	
	def constructor():
		# call superclass constructur to be failsafe :)
		super.constructor();
		setup();

	# overwrite english names with german ones
	def setup():
		self.map.set( "LTANK"  , "Leichter Panzer" );
		self.map.set( "MDTANK" , "Kampfpanzer" );
		self.map.set( "HTANK"  , "Kriegspanzer" );